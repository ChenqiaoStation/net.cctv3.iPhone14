{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          // "@components": "./src/components/",
          // "@datas": "./src/datas/",
          // "@images": "./src/images/",
          // "@pages": "./src/pages/",
          // "@types": "./src/types/",
          // "@zustand": "./src/zustand/"
          "@src": "./src/"
        }
      }
    ]
  ]
}
